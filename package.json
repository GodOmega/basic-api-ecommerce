{
  "name": "ecommerce-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:jest && npm run test:e2e",
    "test:jest": "cross-env NODE_ENV=test jest",
    "test:e2e": "cross-env NODE_ENV=test mocha --config ./test/e2e/.mocharc.js --require ./test/e2e/hooks.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src",
    "start": "cross-env NODE_ENV=production node ./src",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ngneat/falso": "^6.2.0",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "jest": "^29.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "npm": "^9.1.2",
    "supertest": "^6.3.1"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.3",
    "sequelize-cli": "^6.5.2"
  }
}
